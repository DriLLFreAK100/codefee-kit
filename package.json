{
  "name": "codefee-kit",
  "version": "0.0.42-alpha.22",
  "description": "A simple UI kit for React projects",
  "main": "dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "outputDir": "./dist",
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "storybook": "start-storybook -p 6006",
    "storybook:docker": "cross-env NODE_OPTIONS='--openssl-legacy-provider' start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "lint": "eslint src/**/*.ts src/**/*.tsx",
    "lint:fix": "eslint src/**/*.ts src/**/*.tsx --fix",
    "prebuild": "rimraf dist",
    "build": "rollup -c",
    "postbuild": "node scripts/resolveTsPaths",
    "build:local": "npm run build && npm pack",
    "build:publish": "npm run build && npm publish",
    "release:patch": "standard-version --release-as patch",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "release:pre:alpha": "standard-version --prerelease alpha"
  },
  "publishConfig": {
    "access": "public"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DriLLFreAK100/codefee-kit.git"
  },
  "keywords": [
    "Codefee",
    "Codefee-Kit",
    "React",
    "React Component Library"
  ],
  "author": "drillfreak100",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DriLLFreAK100/codefee-kit/issues"
  },
  "homepage": "https://github.com/DriLLFreAK100/codefee-kit#readme",
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@rollup/plugin-commonjs": "17.1.0",
    "@rollup/plugin-node-resolve": "11.2.0",
    "@rollup/plugin-typescript": "8.3.0",
    "@storybook/addon-essentials": "6.4.21",
    "@storybook/addon-storysource": "6.4.21",
    "@storybook/addons": "6.4.21",
    "@storybook/react": "6.4.21",
    "@storybook/theming": "6.4.21",
    "@types/lodash-es": "4.17.5",
    "@types/react-dom": "17.0.14",
    "@types/styled-components": "5.1.7",
    "@typescript-eslint/eslint-plugin": "4.17.0",
    "@typescript-eslint/parser": "4.4.0",
    "cross-env": "7.0.3",
    "css-loader": "4.3.0",
    "eslint": "7.10.0",
    "eslint-config-airbnb": "18.2.0",
    "eslint-config-airbnb-typescript": "12.3.1",
    "eslint-import-resolver-typescript": "2.3.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-react": "7.21.3",
    "eslint-plugin-react-hooks": "4.1.2",
    "eslint-plugin-storybook": "0.5.8",
    "file-loader": "6.1.0",
    "husky": "4.3.0",
    "lodash-es": "4.17.21",
    "postcss": "8.1.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "rimraf": "3.0.2",
    "rollup": "2.40.0",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "rollup-plugin-postcss": "4.0.0",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-visualizer": "4.2.0",
    "sass": "1.26.11",
    "sass-loader": "10.0.2",
    "standard-version": "9.3.2",
    "style-loader": "1.3.0",
    "styled-components": "5.2.1",
    "ts-loader": "8.0.7",
    "tslib": "2.1.0",
    "typescript": "4.0.3",
    "webpack": "4.44.2",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0"
  },
  "peerDependencies": {
    "lodash-es": ">=4",
    "react": ">=16.13.1",
    "react-dom": ">=16.13.1",
    "styled-components": ">= 3"
  },
  "resolutions": {
    "styled-components": "^5"
  }
}
