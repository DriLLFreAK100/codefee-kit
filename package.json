{
  "name": "codefee-kit",
  "version": "0.0.42-alpha.29",
  "description": "A simple UI kit for React projects",
  "main": "dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "outputDir": "./dist",
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "storybook": "start-storybook -p 6006",
    "storybook:docker": "cross-env NODE_OPTIONS='--openssl-legacy-provider' start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint 'src/**/*.{js,jsx,ts,tsx}' --fix",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "prebuild": "rimraf dist",
    "build": "rollup -c",
    "postbuild": "node scripts/resolveTsPaths",
    "build:local": "npm run build && npm pack",
    "build:publish": "npm run build && npm publish",
    "release:patch": "standard-version --release-as patch",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "release:pre:alpha": "standard-version --prerelease alpha",
    "prepare": "husky install"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DriLLFreAK100/codefee-kit.git"
  },
  "keywords": [
    "Codefee",
    "Codefee-Kit",
    "React",
    "React Component Library"
  ],
  "author": "drillfreak100",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DriLLFreAK100/codefee-kit/issues"
  },
  "homepage": "https://github.com/DriLLFreAK100/codefee-kit#readme",
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@rollup/plugin-commonjs": "17.1.0",
    "@rollup/plugin-node-resolve": "11.2.0",
    "@rollup/plugin-typescript": "8.3.0",
    "@storybook/addon-essentials": "6.5.13",
    "@storybook/addon-storysource": "6.5.13",
    "@storybook/addons": "6.5.13",
    "@storybook/react": "6.5.13",
    "@storybook/theming": "6.5.13",
    "@testing-library/react": "^12.1.5",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/lodash-es": "4.17.5",
    "@types/react-dom": "17.0.14",
    "@types/styled-components": "5.1.7",
    "@typescript-eslint/eslint-plugin": "4.17.0",
    "@typescript-eslint/parser": "4.4.0",
    "@vitejs/plugin-react": "^2.2.0",
    "@vitest/coverage-c8": "^0.25.2",
    "cross-env": "7.0.3",
    "css-loader": "4.3.0",
    "eslint": "7.32.0",
    "eslint-config-airbnb": "18.2.0",
    "eslint-config-airbnb-typescript": "12.3.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "2.3.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "7.21.3",
    "eslint-plugin-react-hooks": "4.1.2",
    "eslint-plugin-storybook": "0.6.7",
    "file-loader": "6.1.0",
    "husky": "^8.0.0",
    "jest-environment-jsdom": "^29.3.1",
    "lint-staged": "^13.0.3",
    "lodash-es": "4.17.21",
    "postcss": "8.1.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "rimraf": "3.0.2",
    "rollup": "2.40.0",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "rollup-plugin-postcss": "4.0.0",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-visualizer": "4.2.0",
    "sass": "1.26.11",
    "sass-loader": "10.0.2",
    "standard-version": "9.3.2",
    "style-loader": "1.3.0",
    "styled-components": "5.2.1",
    "ts-loader": "8.0.7",
    "tslib": "2.4.1",
    "typescript": "4.8.4",
    "typescript-plugin-css-modules": "^4.1.1",
    "vite": "^3.2.3",
    "vite-tsconfig-paths": "^3.5.2",
    "vitest": "^0.25.2",
    "webpack": "4.44.2",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0"
  },
  "peerDependencies": {
    "lodash-es": ">=4",
    "react": ">=16.13.1",
    "react-dom": ">=16.13.1",
    "styled-components": ">=3"
  },
  "resolutions": {
    "styled-components": "^5"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{md,html}": "prettier --write"
  }
}
